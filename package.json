{
  "name": "supertranslator",
  "displayName": "SuperTranslator",
  "description": "Just a simple translator that uses the Microsoft Translator API.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:supertranslator.Translate",
    "onCommand:supertranslator.SetAPIkey"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "supertranslator.Translate",
        "title": "Translate"
      },
      {
        "command": "supertranslator.SetAPIkey",
        "title": "Set API key"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
